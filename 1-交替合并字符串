class Solution:
  def mergeAlternately(self, word1: str, word2: str)->str:#->str意思是输出为字符串
    i=j=0
    n,m=len(word1),len(word2)
    ans=[]
    while i<n or j<m:
      if i<n:
        ans.append(word1[i])
        i +=1
      if j<m:
        ans.append(word2[j])
        j +=1
    return "".join(ans)
      
